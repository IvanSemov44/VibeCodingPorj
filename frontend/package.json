{
  "name": "vibecode-full-stack-starter-kit-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "watch:fix": "node ./scripts/watch-and-fix.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --reporter default",
    "test:coverage": "vitest run --coverage",
    "find:unused-exports": "ts-prune",
    "find:unused-deps": "depcheck",
    "find:unused-files": "unimported",
    "find:unused": "npm run find:unused-exports && npm run find:unused-deps && npm run find:unused-files",
    "check": "npm run typecheck && npm run format:check && npm run lint && npm run test:ci",
    "check:all": "npm run typecheck && npm run format:check && npm run lint && npm run test:ci && npm run build"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@reduxjs/toolkit": "^2.5.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-virtual": "^3.13.13",
    "formik": "^2.4.9",
    "next": "15.5.7",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",

    "react-redux": "^9.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^1.0.0",
    "autoprefixer": "^10.4.22",
    "axe-core": "^4.11.0",
    "depcheck": "^1.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",

    "jsdom": "^22.1.0",
    "lint-staged": "^13.0.0",
    "msw": "^1.2.1",
    "postcss": "^8.5.6",
    "@typescript-eslint/parser": "^5.59.5",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.4.19",
    "ts-prune": "^0.10.3",
    "typescript": "^5",
    "typescript-plugin-css-modules": "^5.2.0",
    "unimported": "^1.31.0",
    "vite": "^5.0.0",
    "vitest": "^1.2.4"
  },
  "vitest": {
    "setupFiles": [
      "./tests/vitest.setup.ts"
    ],
    "environment": "jsdom"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}
