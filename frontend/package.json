{
  "name": "vibecode-full-stack-starter-kit-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --ignore-pattern '.next/**'",
    "lint:fix": "eslint . --ext .ts,.tsx --ignore-pattern '.next/**' --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "watch:fix": "node ./scripts/watch-and-fix.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --reporter default",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "@tanstack/react-query": "^5.90.12",
    "formik": "^2.4.9",
    "next": "15.5.7",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-redux": "^9.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^1.0.0",
    "autoprefixer": "^10.4.22",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^22.1.0",
    "msw": "^1.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^5.0.0",
    "vitest": "^1.2.4"
  },
  "vitest": {
    "setupFiles": [
      "./tests/vitest.setup.ts"
    ],
    "environment": "jsdom"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}
